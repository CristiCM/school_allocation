<% content_for :main_classes, 'container mx-auto mt-25 px-5' %>

<% if !current_user.nil? && current_user.role == "student" %>
    <%= render 'student_how_to' %>
<% elsif !current_user.nil? && current_user.role == "admin" %>
    <%= render 'admin_how_to' %>
<%else%>
    <%= render 'unauthorized_home_page' %>
<% end %> 
